@model List<Alumno>

@{
    ViewData["Title"] = "Información Alumno";
    Layout = "_Layout";
}

<h1>@ViewData["Title"]</h1>

<strong>Desde la vista parcial</strong>
<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var alumno in Model)
        {
            <tr>
                <td>@alumno.Nombre</td>
                <td>@alumno.UniqueId</td>
            </tr>
        }
    </tbody>
</table>



<h3>Fecha: @ViewBag.Fecha</h3>
