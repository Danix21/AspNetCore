@model Alumno

@{
    ViewData["Title"] = "Información Alumno";
    Layout = "_Layout";
}

<h1>@ViewData["Title"]</h1>

<strong>Desde la vista normal</strong>
<p>
    Nombre: @Model.Nombre <br/>
    Id: @Model.UniqueId
</p>

<strong>Desde la vista parcial</strong>
<partial name="ObjEscuelaSimple" />

<h3>Fecha: @ViewBag.Fecha</h3>
