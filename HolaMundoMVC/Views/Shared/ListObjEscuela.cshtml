@*
@model List<ObjetoEscuelaBase>
*@

<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var escuela in Model)
        {
            <tr>
                <td>@escuela.Nombre</td>
                <td>@escuela.UniqueId</td>
            </tr>
        }
    </tbody>
</table>